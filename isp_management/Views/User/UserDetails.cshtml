@model isp_management.Models.UserTB
@{
    Layout = "~/Views/Shared/_layout.cshtml";
}
<section id="main-content" class="">
    <section class="wrapper">
        <!-- page start-->
        <!-- page start-->
        <div class="row">

            <div class="col-lg-12">
                <section class="panel">
                    <header class="panel-heading">
                        User Details
                    </header>
                    <div class="table-responsive panel col-md-8">
                        <table class="table">
                            <tbody>

                                <tr>
                                    <td class="text-primary text-right" width="30%">FullName: </td>
                                    <td style="font-weight:bold">@Model.FullName</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">UserName: </td>
                                    <td style="font-weight:bold">@Model.UserName</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Password: </td>
                                    <td style="font-weight:bold">@Model.Password</td>
                                </tr>


                                <tr>
                                    <td class="text-primary text-right" width="30%">Email: </td>
                                    <td style="font-weight:bold">@Model.Email</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Mobile1: </td>
                                    <td style="font-weight:bold">@Model.Mobile1</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Mobile2: </td>
                                    <td style="font-weight:bold">@Model.Mobile2</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Gender: </td>
                                    <td style="font-weight:bold">@Model.Gender_Id</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">National: </td>
                                    <td style="font-weight:bold">@Model.National_Id</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Address: </td>
                                    <td style="font-weight:bold">@Model.Address</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">Remarks: </td>
                                    <td style="font-weight:bold">@Model.Remarks</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">UserStatus: </td>
                                    <td style="font-weight:bold">@Model.UserStatus_Id</td>
                                </tr>
                                <tr>
                                    <td class="text-primary text-right" width="30%">UserType: </td>
                                    <td style="font-weight:bold">@Model.UserType_Id</td>
                                </tr>
                               
                            </tbody>
                        </table>
                        <div class="form-group" style="text-align:center">

                            <a href="~/User/Edit?id=@Model.Id" class="btn btn-success">Edit</a>
                            <a href="~/User/Search" class="btn btn-info">Back</a>

                        </div>
                    </div>
                </section>

            </div>
        </div>

        <!-- page end-->
    </section>
</section>